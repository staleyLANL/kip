#!/bin/bash

echo "compiling with m..."
./m
ls -l s
echo


# ------------------------
# operators: unary
# ------------------------

echo "not"
/usr/bin/time -f '   %e' s -not              50 -render 40 -timing
/usr/bin/time -f '   %e' s -not              50 -render 40 -timing
/usr/bin/time -f '   %e' s -not              50 -render 40 -timing


# ------------------------
# operators: binary
# ------------------------

echo "and"
/usr/bin/time -f '   %e' s -and          250000 -render 40 -timing
/usr/bin/time -f '   %e' s -and          250000 -render 40 -timing
/usr/bin/time -f '   %e' s -and          250000 -render 40 -timing

echo "cut"
/usr/bin/time -f '   %e' s -cut          250000 -render 40 -timing
/usr/bin/time -f '   %e' s -cut          250000 -render 40 -timing
